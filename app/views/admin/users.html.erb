<h1>Admin Users</h1>
<table>
  <tr>
    <th width="30%">Username</th>
    <th width="20%">Email</th>
    <!-- <th width="20%">Last Signed In On</th> -->
  </tr>
  <% for user in @users %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Delete', user, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>
